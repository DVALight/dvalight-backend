generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id  Int @id @default(autoincrement())
  email String @unique
  name String?
  password String

  @@map("users")
}

model Devices {
  id  Int @id @default(autoincrement())
  state Boolean

  @@map("devices")
}
